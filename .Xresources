! font
*font: monospace:size=21,antialias=true
*boldFont: monospace:bold:size=21,antialias=true

Xcursor.theme: Qogir
Xcursor.size:  32

*fading:                10
*fadeColor:             #000000
*pointerColorBackground:#2B2C2B
*pointerColorForeground:#16A085

!! Transparency (0-1):
*.alpha: 0.8

/* name		dark	light */
/* black	0	8 */
/* red		1	9 */
/* green	2	10 */
/* yellow	3	11 */
/* blue		4	12 */
/* purple	5	13 */
/* cyan		6	14 */
/* white	7	15 */

