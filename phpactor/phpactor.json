{
  "indexer.exclude_patterns": [
    "/vendor/**/Tests/**/*",
    "/vendor/**/tests/**/*",
    "/vendor/composer/**/*",
    "/extensions/**/*",
    "/tests/**/*"
  ],
  "indexer.stub_paths": [
    "/home/bjaskulski/Templates/WordPress/wp-admin",
    "/home/bjaskulski/Templates/WordPress/wp-includes",
    "/home/bjaskulski/Plugins/woocommerce/"
  ],
  "composer.autoloader_path": [
    "%project_root%/vendor/autoload.php"
  ],
  "completion_worse.experimantal": true,
  "language_server.profile": false,
  "language_server.trace": false,
  "language_server_phpstan.enabled": true,
  "language_server_worse_reflection.inlay_hints.enable": true,
